<div class="container">
  <br>
  <div class="d-flex justify-content-center">
    <h3>
      <%= @monday.strftime("%D") %> - <%= @monday.next_day(6).strftime("%D") %>
    </h3>
  </div>
  <div class="cal">
    <div class="cal-item">
      <h4>Start Time</h4>
    </div>
    <div class="cal-item">
      <h4>Monday</h4>
    </div>
    <div class="cal-item">
      <h4>Tuesday</h4>
    </div>
    <div class="cal-item">
      <h4>Wednesday</h4>
    </div>
    <div class="cal-item">
      <h4>Thursday</h4>
    </div>
    <div class="cal-item">
      <h4>Friday</h4>
    </div>
    <div class="cal-item">
      <h4>Saturday</h4>
    </div>
    <div class="cal-item">
      <h4>Sunday</h4>
    </div>
    <%= render 'cal_row', start_time: "00:00", end_time: "07:59", time_slot: 0 %>
    <%= render 'cal_row', start_time: "08:00", end_time: "08:59", time_slot: 1 %>
    <%= render 'cal_row', start_time: "09:00", end_time: "09:59", time_slot: 2 %>
    <%= render 'cal_row', start_time: "10:00", end_time: "10:59", time_slot: 3 %>
    <%= render 'cal_row', start_time: "11:00", end_time: "11:59", time_slot: 4 %>
    <%= render 'cal_row', start_time: "12:00", end_time: "12:59", time_slot: 5 %>
    <%= render 'cal_row', start_time: "13:00", end_time: "13:59", time_slot: 6 %>
    <%= render 'cal_row', start_time: "14:00", end_time: "14:59", time_slot: 7 %>
    <%= render 'cal_row', start_time: "15:00", end_time: "15:59", time_slot: 8 %>
    <%= render 'cal_row', start_time: "16:00", end_time: "16:59", time_slot: 9 %>
    <%= render 'cal_row', start_time: "17:00", end_time: "17:59", time_slot: 10 %>
    <%= render 'cal_row', start_time: "18:00", end_time: "18:59", time_slot: 11 %>
    <%= render 'cal_row', start_time: "19:00", end_time: "19:59", time_slot: 12 %>
    <%= render 'cal_row', start_time: "20:00", end_time: "20:59", time_slot: 13 %>
    <%= render 'cal_row', start_time: "21:00", end_time: "21:59", time_slot: 14 %>
    <%= render 'cal_row', start_time: "22:00", end_time: "23:59", time_slot: 15 %>

  </div>
</div>
