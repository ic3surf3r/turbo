<%= simple_form_for([club, event], method: :post, url: events_path) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.association :team, collection: club.teams %>
  <%= f.input :start_time, as: :time %>
  <%= f.input :duration, collection: ['0:15', '0:30', '0:45', '1:00', '1:15', '1:30', '1:45', '2:00', '2:15', '2:30', '2:45', '3:00', '3:15', '3:30', '3:45', '4:00']  %>
  <%= f.submit "Submit Event", class:"btn btn-secondary"%>
<% end %>
